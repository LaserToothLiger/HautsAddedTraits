<?xml version="1.0" encoding="utf-8" ?>
<Patch>
	<Operation Class="PatchOperationFindMod">
		<mods>
			<li>Dubs Rimatomics</li>
		</mods>
		<match Class="PatchOperationSequence">
			<operations>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/GameConditionDef[defName="RadioactiveFallout"]</xpath>
					<value>
						<li Class="HautsTraits.CatastrophistLikedCondition" />
					</value>
				</li>
				<li Class="PatchOperationAddModExtension">
					<xpath>/Defs/GameConditionDef[defName="NuclearFallout"]</xpath>
					<value>
						<li Class="HautsTraits.CatastrophistLikedCondition" />
					</value>
				</li>
			</operations>
		</match>
	</Operation>
</Patch>